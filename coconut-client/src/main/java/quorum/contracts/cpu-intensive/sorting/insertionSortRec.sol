contract insertionSortRec {

    event sorted(string eventType, string signature);

    function insertsort(int[] memory A, int l, int r) public {
        if (l < r) {
            insertsort(A, l, r - 1);
            insert(A, l, r);
        }
    }

    function main(int[] memory A, int l, int r, string memory sig) public {
        insertsort(A, l, r);
        emit sorted("sort/insertionSortRec", sig);
    }

    function insert(int[] memory A, int l, int r) internal {
        if (l < r) {
            if (A[uint(r - 1)] > A[uint(r)]) {
                exchange(A, r - 1, r);
            }
            insert(A, l, r - 1);
        }
    }

    function exchange(int[] memory A, int q, int i) internal pure {
        int tmp = A[uint(q)];
        A[uint(q)] = A[uint(i)];
        A[uint(i)] = tmp;

    }

}

