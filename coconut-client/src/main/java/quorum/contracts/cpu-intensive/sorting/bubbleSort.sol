contract bubbleSort {

    event sorted(string eventType, string signature);

    function bubblesort(int[] memory A, int l, int r, string memory sig) public {
        for (int i = r; i >= l + 1; i--) {
            for (int j = l; j <= i - 1; j++) {
                if (A[uint(j)] > A[uint(j + 1)]) {
                    exchange(A, j, j + 1);
                }
            }
        }
        emit sorted("sort/bubbleSort", sig);
    }

    function exchange(int[] memory A, int q, int i) internal pure {
        int tmp = A[uint(q)];
        A[uint(q)] = A[uint(i)];
        A[uint(i)] = tmp;

    }

}

