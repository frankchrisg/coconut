contract smallBank {
    mapping(string => int) entries;
    mapping(string => bool) exists;

    event balanceEvent(string eventType, string signature);
    event amalgamateEvent(string eventType, string signature);
    event writeCheckEvent(string eventType, string signature);
    event depositCheckingEvent(string eventType, string signature);
    event createAccountEvent(string eventType, string signature);
    event sendPaymentEvent(string eventType, string signature);
    event transactSavingsEvent(string eventType, string signature);
    event errorEvent(string eventType, string signature);

    string constant checkingSuffix = "_checking";
    string constant savingsSuffix = "_savings";

    function writeCheck(string memory acctId, int amount, string memory sig) public {

        if (!(exists[getSavings(acctId)]) || !(exists[getChecking(acctId)]) || amount < 0) {
            emit errorEvent("writeCheck/error", sig);
            return;
        }

        int savings = entries[getSavings(acctId)];
        int checks = entries[getChecking(acctId)];
        int total = savings + checks;

        if (total < amount) {
            entries[getChecking(acctId)] = checks - amount - 1;
        } else {
            entries[getChecking(acctId)] = checks - amount;
        }
        emit writeCheckEvent("writeCheck", sig);
    }

    function depositChecking(string memory acctId, int amount, string memory sig) public {
        if (!(exists[getChecking(acctId)])) {
            emit errorEvent("depositChecking/error", sig);
            return;
        }
        entries[getChecking(acctId)] += amount;
        emit depositCheckingEvent("depositChecking", sig);
    }

    function transactSavings(string memory acctId, int amount, string memory sig) public {

        if (!(exists[getSavings(acctId)])) {
            emit errorEvent("transactSavings/error", sig);
            return;
        }

        int balance = entries[getSavings(acctId)] - amount;
        if (balance < 0) {
            emit errorEvent("transactSavings/error/balance", sig);
            return;
        }
        entries[getSavings(acctId)] -= amount;
        emit transactSavingsEvent("transactSavings", sig);
    }

    function sendPayment(string memory sender, string memory destination, int amount, string memory sig) public {

        if (!(exists[getChecking(sender)]) || !(exists[getChecking(destination)]) || !(exists[getSavings(sender)]) || !(exists[getSavings(destination)])) {
            emit errorEvent("sendPayment/error", sig);
            return;
        }

        int checkingBalanceSender = entries[getChecking(sender)];

        if (checkingBalanceSender < amount) {
            emit errorEvent("sendPayment/error/checkingBalanceSender", sig);
            return;
        }
        entries[getChecking(sender)] -= amount;
        entries[getChecking(destination)] += amount;
        emit sendPaymentEvent("sendPayment", sig);
    }

    function balance(string memory acctId, string memory sig) public /*returns (int t)*/ {
        if (!(exists[getSavings(acctId)]) || !(exists[getChecking(acctId)])) {
            emit errorEvent("balance/error", sig);
            //return 0;
            return;
        }
        int savings = entries[getSavings(acctId)];
        int checkings = entries[getChecking(acctId)];

        int total = savings + checkings;

        //string memory balanceAsString = intToStr(total);
        sig = string(abi.encodePacked(sig, " __balance_event:", "-balance-", int2str(total), "-acctId-", acctId));

        emit balanceEvent("balance", sig);
        //return total;
    }

    function amalgamate(string memory acctId0, string memory acctId1, string memory sig) public {

        if (!(exists[getChecking(acctId0)]) || !(exists[getChecking(acctId1)]) || !(exists[getSavings(acctId0)]) || !(exists[getSavings(acctId1)])) {
            emit errorEvent("amalgamate/error", sig);
            return;
        }

        int savingsBalance = entries[getSavings(acctId0)];
        int checkingsBalance = entries[getChecking(acctId0)];

        int total = savingsBalance + checkingsBalance;
        //assert(total >= 0);

        entries[getChecking(acctId0)] = 0;
        entries[getSavings(acctId0)] = 0;
        entries[getChecking(acctId1)] += total;

        emit amalgamateEvent("amalgamate", sig);
    }

    function createAccount(string memory acctId, int checking, int savings, string memory sig) public {
        exists[getSavings(acctId)] = true;
        exists[getChecking(acctId)] = true;
        entries[getChecking(acctId)] = checking;
        entries[getSavings(acctId)] = savings;
        emit createAccountEvent("createAccount", sig);
    }

    function getSavings(string memory a) private pure returns (string memory) {
        return string(abi.encodePacked(a, savingsSuffix));
    }

    function getChecking(string memory a) private pure returns (string memory) {
        return string(abi.encodePacked(a, checkingSuffix));
    }

    function int2str(int i) internal pure returns (string memory ret){
        if (i == 0) return "0";
        bool negative = i < 0;
        uint j = uint(negative ? -i : i);
        uint l = j;
        uint len;
        while (j != 0){
            len++;
            j /= 10;
        }
        if (negative) ++len;
        bytes memory bstr = new bytes(len);
        uint k = len - 1;
        while (l != 0){
            bstr[k--] = byte(uint8(48 + l % 10));
            l /= 10;
        }
        if (negative) {
            bstr[0] = '-';
        }
        return string(bstr);
    }

}

